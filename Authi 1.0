      "source": [
        "# 1. Drop columns in df_conditions and df_treatments that contain only null values.\n",
        "df_conditions.dropna(axis=1, how='all', inplace=True)\n",
        "df_treatments.dropna(axis=1, how='all', inplace=True)\n",
        "\n",
        "# 2. In df_conditions, rename the column 'CHRONIC CONDITIONS' to 'Condition'.\n",
        "df_conditions.rename(columns={'CHRONIC CONDITIONS': 'Condition'}, inplace=True)\n",
        "\n",
        "# 3. In df_treatments, identify the row that contains the actual column headers.\n",
        "# 4. Set this identified row as the new header for df_treatments and drop the rows above it.\n",
        "header_row = df_treatments[df_treatments['CONDITION'] == 'CONDITION'].index[0]\n",
        "df_treatments.columns = df_treatments.iloc[header_row]\n",
        "df_treatments = df_treatments[header_row + 1:].reset_index(drop=True)\n",
        "\n",
        "# 5. Remove any rows from df_treatments where the 'CONDITION' column is null.\n",
        "df_treatments.dropna(subset=['CONDITION'], inplace=True)\n",
        "\n",
        "# 6. Standardize the 'Condition' column in both dataframes.\n",
        "df_conditions['Condition'] = df_conditions['Condition'].str.strip().str.lower()\n",
        "df_treatments['CONDITION'] = df_treatments['CONDITION'].str.strip().str.lower()\n",
        "\n",
        "# 7. Inspect the dataframes again to verify the changes.\n",
        "display(df_conditions.head())\n",
        "display(df_treatments.head())"
