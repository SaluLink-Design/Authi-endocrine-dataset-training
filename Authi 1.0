      "source": [
        "def retrieve_treatment_info(df, condition_name):\n",
        "    \"\"\"\n",
        "    Retrieves and displays diagnostic and ongoing management treatment information\n",
        "    for a given medical condition from a structured DataFrame.\n",
        "\n",
        "    Args:\n",
        "        df: pandas DataFrame with columns 'condition', 'diagnostic_basket',\n",
        "            and 'ongoing_management_basket'.\n",
        "        condition_name: string, the name of the medical condition to query.\n",
        "    \"\"\"\n",
        "    # Ensure the condition name is in lowercase for matching\n",
        "    condition_name = condition_name.lower()\n",
        "\n",
        "    # Filter the DataFrame to find the matching condition\n",
        "    filtered_df = df[df['condition'] == condition_name]\n",
        "\n",
        "    # Check if a matching condition was found\n",
        "    if not filtered_df.empty:\n",
        "        # Extract the baskets from the first matching row\n",
        "        diagnostic_basket = filtered_df.iloc[0]['diagnostic_basket']\n",
        "        ongoing_management_basket = filtered_df.iloc[0]['ongoing_management_basket']\n",
        "\n",
        "        print(f\"--- Treatment Information for: {condition_name.capitalize()} ---\")\n",
        "\n",
        "        # Display Diagnostic Basket\n",
        "        print(\"\\nDiagnostic Basket:\")\n",
        "        if diagnostic_basket:\n",
        "            for item in diagnostic_basket:\n",
        "                print(f\"  - Procedure: {item.get('procedure', 'N/A')}\")\n",
        "                print(f\"    Code: {item.get('code', 'N/A')}\")\n",
        "                print(f\"    Coverage Number: {item.get('coverage_number', 'N/A')}\")\n",
        "        else:\n",
        "            print(\"  No diagnostic information available.\")\n",
        "\n",
        "        # Display Ongoing Management Basket\n",
        "        print(\"\\nOngoing Management Basket:\")\n",
        "        if ongoing_management_basket:\n",
        "            for item in ongoing_management_basket:\n",
        "                print(f\"  - Procedure: {item.get('procedure', 'N/A')}\")\n",
        "                print(f\"    Code: {item.get('code', 'N/A')}\")\n",
        "                print(f\"    Coverage Number: {item.get('coverage_number', 'N/A')}\")\n",
        "        else:\n",
        "            print(\"  No ongoing management information available.\")\n",
        "\n",
        "        print(\"-\" * (len(condition_name) + 34)) # Print a separator line\n",
        "    else:\n",
        "        print(f\"Condition '{condition_name.capitalize()}' not found in the dataset.\")\n",
        "\n",
        "# Test the function with example condition names\n",
        "retrieve_treatment_info(df_structured, \"hypothyroidism\")\n",
        "retrieve_treatment_info(df_structured, \"addison's disease\")\n",
        "retrieve_treatment_info(df_structured, \"non-existent condition\")\n",
        "retrieve_treatment_info(df_structured, \"diabetes insipidus\")\n"
